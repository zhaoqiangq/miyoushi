page{
  height: 100%;
  background: #f8f8f8;
}
.content {
  background: #fff;
  padding: 70rpx 30rpx;
  text-align: center;
  font-size: 40rpx;
  color: #333; 
  border-top:1rpx solid #e2e2e2;
  line-height: 60rpx;
}
.content view{
  font-size: 30rpx;
  color: #99804d;
}
.content image{
